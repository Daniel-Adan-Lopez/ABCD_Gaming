                         tfmri_mid_all_loss.pos.vs.neg.feedback_beta_subcort.aseg_pallidum.lh+(1|abcd_site/rel_family_id),
                       chains=4, cores=4, warmup=1000, iter=2000, family = negbinomial(), 
                       control=list(adapt_delta=0.80), seed=1234, backend="cmdstanr", threads = threading(12), data=ABCD_VG_Positive) 

