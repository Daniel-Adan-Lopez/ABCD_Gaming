library(dplyr)
#summarizing distributions for the sensitvity analysis

ABCD_VG_Positive %>% 
  group_by(sex) %>% 
  summarize(mean = mean(VGTotal_zero, na.rm=TRUE),
            #         summarize(median = median(pps_y_ss_severity_score, na.rm=TRUE),
            (sd=sd(VGTotal_zero, na.rm=TRUE))) 

ABCD_VG_Positive %>% 
  group_by(race_ethnicity) %>% 
  summarize(mean = mean(VGTotal_zero, na.rm=TRUE),
            #         summarize(median = median(pps_y_ss_severity_score, na.rm=TRUE),
            (sd=sd(VGTotal_zero, na.rm=TRUE))) 

ABCD_VG_Positive %>% 
  group_by(hh_combined) %>% 
  summarize(mean = mean(VGTotal_zero, na.rm=TRUE),
            #         summarize(median = median(pps_y_ss_severity_score, na.rm=TRUE),
            (sd=sd(VGTotal_zero, na.rm=TRUE))) 

ABCD_VG_Positive %>% 
  group_by(high.educ.bl) %>% 
  summarize(mean = mean(VGTotal_zero, na.rm=TRUE),
            #         summarize(median = median(pps_y_ss_severity_score, na.rm=TRUE),
            (sd=sd(VGTotal_zero, na.rm=TRUE))) 

ABCD_VG_Positive %>% 
  group_by(married.bl) %>% 
  summarize(mean = mean(VGTotal_zero, na.rm=TRUE),
            #         summarize(median = median(pps_y_ss_severity_score, na.rm=TRUE),
            (sd=sd(VGTotal_zero, na.rm=TRUE))) 

ABCD_VG_Positive %>% 
  group_by(neighborhood_crime_y) %>% 
  summarize(mean = mean(VGTotal_zero, na.rm=TRUE),
            #         summarize(median = median(pps_y_ss_severity_score, na.rm=TRUE),
            (sd=sd(VGTotal_zero, na.rm=TRUE))) 

