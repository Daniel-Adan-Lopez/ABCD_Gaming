#plotting

library(bayesplot)


conditional_effects(model_NACC_VG_hurdle_5 , effects="tfmri_mid_all_antic.large.loss.vs.neutral_beta_subcort.aseg_accumbens.area.lh")
library(ggplot2)
### THE CODE BELOW CUSTOMIZES THE CONDITIONAL_EFFECTS PLOT
 
gg <- plot(conditional_effects(model_NACC_VG_hurdle_5 , effects="tfmri_mid_all_antic.large.loss.vs.neutral_beta_subcort.aseg_accumbens.area.lh", re_formula=NULL))

 

#get each plot separately
plot1 <- gg[[1]]
plot2 <- gg[[2]]
plot3 <- gg[[3]]
plot6 <- gg[[6]]

cbPalette <- c("#999999", "#E69F00", "#56B4E9", "#009E73", "#F0E442", "#0072B2", "#D55E00", "#CC79A7")

# The palette with black:
cbbPalette <- c("#000000", "#E69F00", "#56B4E9", "#009E73", "#F0E442", "#0072B2", "#D55E00", "#CC79A7")

plot6_1 <- plot1 +
  labs(x = "Anticipation of a large loss vs. neutral condition - nucleus accumbens", y = "VGAQ score") +
  #ggtitle("Plot for the association between reward anticipation and VGAQ score")+
   
  theme_classic() +
  theme(panel.grid = element_blank(), 
        text = element_text(size=18), 
        axis.title.x = element_text(margin = margin(t=10,r=0,b=0,l=0)),
        axis.title.y = element_text(margin = margin(t=0,r=10,b=0,l=0)))

p <- plot6_1+theme(
  plot.title = element_text(color="black", size=16, face="bold"))

p+ scale_y_continuous(breaks=seq(0,12,by=1)) 
p
