tfmri_mid_all_antic.large.loss.vs.neutral_beta_subcort.aseg_accumbens.area.lh
model_NACC_VG_hurdle_5 <- brm(VGTotal_zero ~ 1+sex+  interview_age_scaled+race_ethnicity+high.educ.bl+hh_combined+married.bl+
                                neighborhood_crime_y+pmq_y_ss_mean_scaled+ CBCLAttention_Final_scaled +
                                tfmri_mid_all_antic.large.loss.vs.neutral_beta_subcort.aseg_accumbens.area.lh+(1|abcd_site/rel_family_id),
                              chains=4, cores=4, warmup=1000, iter=6000, family = negbinomial(), 
                              control=list(adapt_delta=0.90), seed=1234, backend="cmdstanr", threads = threading(12), data=ABCD_GamingYear2_QC1)

summary(model_NACC_VG_hurdle_5)
#tfmri_mid_all_antic.large.loss.vs.neutral_beta_subcort.aseg_accumbens.area.lh    -0.09      0.04    -0.17    -0.01 1.00     7737     3065
print(model_NACC_VG_hurdle_5, digits=4)
#tfmri_mid_all_antic.large.loss.vs.neutral_beta_subcort.aseg_accumbens.area.lh  -0.0909    0.0400  -0.1698  -0.0126 1.0004    26822     8285

min <- min(ABCD_GamingYear2_QC1$VGTotal_zero)
max <- max(ABCD_GamingYear2_QC1$VGTotal_zero)
pp_check(model_NACC_VG_hurdle_5, type="bars") + 
  xlim(min, max)

pp_check(model_NACC_VG_hurdle_5) + 
  xlim(min, max)

model_NACC_VG_hurdle_linear <- brm(VGTotal_zero ~ 1+sex+  interview_age_scaled+race_ethnicity+high.educ.bl+hh_combined+married.bl+
                                neighborhood_crime_y+pmq_y_ss_mean_scaled+ CBCLAttention_Final_scaled +
                                tfmri_mid_all_antic.large.loss.vs.neutral_beta_subcort.aseg_accumbens.area.lh+(1|abcd_site/rel_family_id),
                              chains=4, cores=4, warmup=1000, iter=6000, 
                              control=list(adapt_delta=0.90), seed=1234, backend="cmdstanr", threads = threading(12), data=ABCD_GamingYear2_QC1)

summary(model_NACC_VG_hurdle_linear)
pp_check(model_NACC_VG_hurdle_linear) + 
  xlim(min, max)

