zero_inflated_negbinomial()
model_NACC_VG_hurdle_5_1 <- brm(VGTotal_zero ~ 1+sex+  interview_age_scaled+race_ethnicity+high.educ.bl+hh_combined+married.bl+
                                neighborhood_crime_y+pmq_y_ss_mean_scaled+ CBCLAttention_Final_scaled +
                                tfmri_mid_all_antic.large.loss.vs.neutral_beta_subcort.aseg_accumbens.area.lh+(1|abcd_site/rel_family_id),
                              chains=4, cores=4, warmup=1000, iter=6000, family = zero_inflated_negbinomial(), 
                              control=list(adapt_delta=0.90), seed=1234, backend="cmdstanr", threads = threading(12), data=ABCD_GamingYear2_QC1)

summary(model_NACC_VG_hurdle_5_1)
#tfmri_mid_all_antic.large.loss.vs.neutral_beta_subcort.aseg_accumbens.area.lh    -0.09      0.04    -0.16    -0.02 1.00    32154    14498
pp_check(model_NACC_VG_hurdle_5_1, type="bars") + 
  xlim(min, max)
